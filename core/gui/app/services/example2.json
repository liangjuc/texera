{
  "type": "object",
  "title" : "Person",
  "id": "person",
  "properties": {
    "properties" : {
      "type" : "string",
      "default" : "Hello WOrld!"
    },
    "name": {
      "type": "string",
      "minLength": 4
    },
    "age": {
      "type": "integer",
      "default": 21,
      "minimum": 18,
      "maximum": 99
    },
    "gender": {
      "type": "string",
      "enum": [
        "male",
        "female"
      ]
    }
  },
  "required" : ["properties", "name" , "gender"]
}
